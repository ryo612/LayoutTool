void pattern(float x, float y, float d, int[][][] COLOR, int num) {
  noStroke();
  for (int i=0; i<8; i++) {
    if (i%2==0) {
      fill(COLOR[num][0][0], COLOR[num][0][1], COLOR[num][0][2]);
    } else if (i%4==1) {
      fill(COLOR[num][1][0], COLOR[num][1][1], COLOR[num][1][2]);
    } else {
      fill(COLOR[num][2][0], COLOR[num][2][1], COLOR[num][2][2]);
    }
    arc(x, y, d, d, radians(0+i*45), radians(45+i*45));
    fill(COLOR[num][0][0], COLOR[num][0][1], COLOR[num][0][2]);
    ellipse(x, y, d/2.5, d/2.5);
  }
}
